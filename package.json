{
  "publisher": "RidwanMahenra",
  "name": "mkalib",
  "displayName": "mkalib",
  "description": "Library Kecerdasan Buatan",
  "version": "0.1.1",
  "icon": "assets/logo/light/light.png",
  "engines": {
    "vscode": "^1.76.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ridwanmahendra/mkalib.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
      "command": "mkalib.helloWorld",
      "title": "Hello World"
      },
      {
        "command": "mkalib.logisticRegression",
        "title": "mkalib: Logistic Regression"
      },
      {
        "command": "mkalib.linearRegression",
        "title": "mkalib: Linear Regression"
      },
      {
        "command": "mkalib.createFile",
        "title": "mkalib: New File"
      },
      {
        "command": "mkalib.createNotebook",
        "title": "mkalib: New Notebook"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
